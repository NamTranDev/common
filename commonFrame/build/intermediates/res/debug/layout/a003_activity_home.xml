        android:layout_height="wrap_content"
        android:text="@string/a003_bt_current" />

    <Button
        android:id="@+id/a003_bt_howto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/a003_bt_howto" />

